            "multidrug@tri": "biocide@tri", // tricosan
            "multidrug@opmH": "biocide@opmH", // tricosan

            "multidrug@sepA": "biocide@sepA", // Benzalkonium, chlorhexidine, acriflavine   
            "multidrug@vmrA": "biocide@vmrA", // 4prime;,6prime;-diamino-2-phenylindole (DAPI), tetraphenylphosphonium chloride (TPPCl), and acriflavine
            "multidrug@smvA": "biocide@smvA", // cationic biocides in several bacterial species and that increased efflux through SmvA can lead to increased chlorhexidine and octenidine tolerance.
