            "multidrug@hsmR": "biocide@hsmR", // resistance only to ethidium bromide and acriflavine, carried by archaea, H. salinarum

            "multidrug@(?=ttg[DEF])": "biocide@", // ttgDEF does not confer antibiotic resistance, only solvent
            "multidrug@(?=vme[GHI])": "biocide@", // vmeGHI does not confer antibiotic resistance, even in full KO strain of V. parahaemolyticus
            "multidrug@(?=mdr[MT])": "biocide@", // bile salt only
            "multidrug@(?=dinF)": "biocide@", // expression of the E. coli dinF gene in E. coli conferred only a slight resistance to bile salts

